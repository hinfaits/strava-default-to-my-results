{
  "manifest_version": 2,

  "name": "Strava: Default to 'My Results'",
  "description": "Show 'My Results' by default when analysing efforts",
  "version": "1.0",

  "content_scripts": [
    {
      "js": ["js/content_script.js"],
      "run_at": "document_end",
      "matches": ["*://*.strava.com/*"]
    }
  ],
  "web_accessible_resources": [
    "js/injected.js"
  ]
}
